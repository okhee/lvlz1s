
<!-- 목록 출력 영역 -->
<table class="table table-horizontal table-bordered">
    <thead class="thead-strong">
    <tr>
        <th>노래 번호</th>
        <th>제목</th>
        <th>앨범명</th>
        <th>파일 id</th>
    </tr>
    </thead>
    <tbody id="tbody">
    {{#songList}}
        <tr>
            <td>{{id}}</td>
            <td>{{songName}}</td>
            <td>{{albumId}}</td>
            <td>{{fileId}}</td>
        </tr>
    {{/songList}}
    </tbody>
</table>

